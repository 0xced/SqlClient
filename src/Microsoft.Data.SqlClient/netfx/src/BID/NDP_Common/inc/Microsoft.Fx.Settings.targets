<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <!--
        This file is used by fx src directory.
     -->

    <Import Project="$(_NTDRIVE)$(_NTROOT)\InternalAPIs\NDP_Common\Inc\Microsoft.Ndp.Settings.targets" />

    <PropertyGroup>
        <NdpFxRoot>$(NdpRoot)\fx</NdpFxRoot>
    </PropertyGroup>

    <PropertyGroup>
        <!-- override OutputPath so we don't copy to the binaries\bin\i386 directory -->
        <OutputPath>$(BinariesDirectory)</OutputPath>

        <!-- Workaround the MSBuild issue for parallel building -->
        <TrackFileAccess>false</TrackFileAccess>
    </PropertyGroup>

</Project>

