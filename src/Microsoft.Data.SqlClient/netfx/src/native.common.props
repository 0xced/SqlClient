<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  
  <PropertyGroup>
      <SolutionDir>$([System.IO.Path]::GetDirectoryName($(MSBuildThisFileDirectory)))\</SolutionDir>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(BinFolder)$(Configuration).$(Platform)\$(MSBuildProjectName)\</OutDir>
    <IntermediateDir>$(ObjFolder)$(Configuration).$(Platform)\$(MSBuildProjectName)\</IntermediateDir>
    <IntDir>$(IntermediateDir)</IntDir>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Configuration)'=='Debug'" Label="Configuration">
    <DebugType>Full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <LinkIncremental>True</LinkIncremental>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'" Label="Configuration">
    <DebugType>Pdbonly</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <LinkIncremental>False</LinkIncremental>
  </PropertyGroup>

  <Target Name="DisplayMessages">  
    <Message Text="Solution Dir = $(SolutionDir)" />  
    <Message Text="Project Extension = $(MSBuildProjectName)" />  
    <Message Text="Bin Dir = $(BinFolder)" />  
    <Message Text="Obj Dir= $(ObjFolder)" />  
  </Target>
  
  <!-- Begin Section Common Properties for all projects -->
  <ItemDefinitionGroup>
    <ClCompile>
      <DiagnosticsFormat>Caret</DiagnosticsFormat>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
      <SuppressStartupBanner>True</SuppressStartupBanner>
      <WarningLevel>Level3</WarningLevel>
      <TreatWarningAsError>True</TreatWarningAsError>
      <OmitFramePointers>False</OmitFramePointers>
      <FavorSizeOrSpeed>Size</FavorSizeOrSpeed>
      <StringPooling>True</StringPooling>
      <MinimalRebuild>False</MinimalRebuild>
      <ExceptionHandling>Sync</ExceptionHandling>
      <RuntimeLibrary>MultiThreadedDLL</RuntimeLibrary>
      <StructMemberAlignment>8Bytes</StructMemberAlignment>
      <BufferSecurityCheck>True</BufferSecurityCheck>
      <FunctionLevelLinking>True</FunctionLevelLinking>
      <TreatWChar_tAsBuiltInType>False</TreatWChar_tAsBuiltInType>
      <ForceConformanceInForLoopScope>False</ForceConformanceInForLoopScope>
      <RuntimeTypeInfo>False</RuntimeTypeInfo>
      <CompileAs>CompileAsCpp</CompileAs>
      <UseFullPaths>True</UseFullPaths>
      <OmitDefaultLibName>True</OmitDefaultLibName>
      <RemoveUnreferencedCodeData>False</RemoveUnreferencedCodeData>
      <SupportJustMyCode>false</SupportJustMyCode>
      <PrecompiledHeader>NotUsing</PrecompiledHeader>
      <CallingConvention>StdCall</CallingConvention>
      <AdditionalOptions>/Zo /d2guard4 /d2guardcfgfuncptr /d2nocfgrngchk</AdditionalOptions>
      <DisableSpecificWarnings>4091;4267;4456;4457;4458;4459;4603;4627;4838;4960;4961;</DisableSpecificWarnings>
      <PreprocessorDefinitions>_CRT_STDIO_LEGACY_WIDE_SPECIFIERS;DEV10;WIN32;NT_UP=1;DEVL=1;_WIN32_WINNT=0x0600;_WIN32_IE=0x0900;WINNT=1;NT_INST=0;CONDITION_HANDLING=1;NTDDI_VERSION=NTDDI_VISTA</PreprocessorDefinitions>
    </ClCompile>
    <Lib>
      <IgnoreAllDefaultLibraries>True</IgnoreAllDefaultLibraries>
      <Verbose>False</Verbose>
      <AdditionalOptions>/IGNORE:4070,4221 %(AdditionalOptions)</AdditionalOptions>
    </Lib>
    </ItemDefinitionGroup> 
    <!-- End Section Common Properties for all projects -->

  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <ClCompile>
      <Optimization>Disabled</Optimization>
      <EnableEnhancedInstructionSet>NoExtensions</EnableEnhancedInstructionSet>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);FPO=1;STD_CALL;_X86_=1;i386=1;</PreprocessorDefinitions>
    </ClCompile>
    <Lib>
      <TargetMachine>MachineX86</TargetMachine>
      <AdditionalOptions>/SUBSYSTEM:CONSOLE,"5.01" %(AdditionalOptions)</AdditionalOptions>
    </Lib>
    <Link>
      <GenerateDebugInformation>True</GenerateDebugInformation>
    </Link>
  </ItemDefinitionGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <ClCompile>
      <Optimization>Full</Optimization>
      <EnableEnhancedInstructionSet>NoExtensions</EnableEnhancedInstructionSet>
      <WholeProgramOptimization>True</WholeProgramOptimization>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);FPO=1;STD_CALL;_X86_=1;i386=1;NDEBUG</PreprocessorDefinitions>
      <DisableSpecificWarnings>4951;4952;4953;%(DisableSpecificWarnings)</DisableSpecificWarnings>
    </ClCompile>
    <Lib>
      <TargetMachine>MachineX86</TargetMachine>
      <AdditionalOptions>/SUBSYSTEM:CONSOLE,"5.01" %(AdditionalOptions)</AdditionalOptions>
    </Lib>
    <Link>
      <EnableCOMDATFolding>True</EnableCOMDATFolding>
      <OptimizeReferences>True</OptimizeReferences>
      <GenerateDebugInformation>True</GenerateDebugInformation>
    </Link>
  </ItemDefinitionGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <ClCompile>
      <Optimization>Disabled</Optimization>
       <PreprocessorDefinitions>%(PreprocessorDefinitions);_AMD64_;_AMD64_SIMULATOR_;_AMD64_SIMULATOR_PERF_;_AMD64_WORKAROUND_;_WIN64;AMD64;_SKIP_IF_SIMULATOR_</PreprocessorDefinitions>
    </ClCompile>
    <Lib>
      <TargetMachine>MachineX64</TargetMachine>
      <AdditionalOptions>/SUBSYSTEM:CONSOLE,"6.00" %(AdditionalOptions)</AdditionalOptions>
    </Lib>
    <Link>
      <GenerateDebugInformation>True</GenerateDebugInformation>
    </Link>
  </ItemDefinitionGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <ClCompile>
      <Optimization >Full</Optimization>
      <WholeProgramOptimization>True</WholeProgramOptimization>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);_AMD64_;_AMD64_SIMULATOR_;_AMD64_SIMULATOR_PERF_;_AMD64_WORKAROUND_;_WIN64;AMD64;_SKIP_IF_SIMULATOR_;NDEBUG</PreprocessorDefinitions>
      <DisableSpecificWarnings>4951;4952;4953;%(DisableSpecificWarnings)</DisableSpecificWarnings>
    </ClCompile>
    <Lib>
      <TargetMachine>MachineX64</TargetMachine>
      <AdditionalOptions>/SUBSYSTEM:CONSOLE,"6.00" %(AdditionalOptions)</AdditionalOptions>
    </Lib>
    <Link>
      <EnableCOMDATFolding>True</EnableCOMDATFolding>
      <OptimizeReferences>True</OptimizeReferences>
      <GenerateDebugInformation>True</GenerateDebugInformation>
    </Link>
  </ItemDefinitionGroup>
  
  <ItemGroup />
</Project>